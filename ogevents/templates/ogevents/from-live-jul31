<script>
function changeFontSize(xdate) {
if (document.getElementById(xdate).style.fontSize != 'xx-large'){
   document.getElementById(xdate).style.fontSize = 'xx-large'}
else {
   document.getElementById(xdate).style.fontSize = 'large'}
}
</script>
